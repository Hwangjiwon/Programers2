package NorS;


public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println(solution("d2dd3d"));
	}

	public static boolean solution(String s) {
		boolean answer = true;
		
		if(s.length() != 4) answer = false;
		if(s.length() != 6) answer = false;
		
		return answer;
	}
}
