package MidChar;

class Main {

	public static String main(String s) {
		
		char[] chaArr = new char[s.length()];
		
		for(int i = 0; i < s.length(); i++)
			chaArr[i] = s.charAt(i);
		
		if ((s.length() % 2 ) == 0)
			System.out.println(chaArr[s.length()/2+1]);
		else
			System.out.println(chaArr[s.length()/2-1]+chaArr[s.length()/2+1] );
		return s;
	}

}
