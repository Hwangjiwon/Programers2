package Divisor;

import java.util.Arrays;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] arr = { 5, 9, 7, 10 };

		solution(arr, 5);
	}

	public static int[] solution(int[] arr, int divisor) {
		int[] answer = {};
		int j = 0;
		for (int i = 0; i < arr.length; i++) {
			if (arr[i] % divisor == 0) {
				answer[j] = arr[i];
				j++;
			}
		}

		Arrays.sort(answer);

		return answer;
	}
}
